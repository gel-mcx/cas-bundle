<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="cas_bundle_homepage" path="/homepage">
        <default key="_controller">drupol\CasBundle\Controller\CasBundle\Homepage::__invoke</default>
    </route>

    <route id="cas_bundle_login" path="/login">
        <default key="_controller">drupol\CasBundle\Controller\CasBundle\Login::__invoke</default>
    </route>

    <route id="cas_bundle_logout" path="/logout">
        <default key="_controller">drupol\CasBundle\Controller\CasBundle\Logout::__invoke</default>
    </route>

    <route id="cas_bundle_proxy_callback" path="/proxy/callback">
        <default key="_controller">drupol\CasBundle\Controller\CasBundle\ProxyCallback::__invoke</default>
    </route>
</routes>
